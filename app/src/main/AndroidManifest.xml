<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="totoro.project.interaction.interactiontestproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:screenOrientation="fullSensor"
        android:configChanges="orientation|screenSize"
        android:theme="@style/AppTheme">
        <activity android:name="totoro.project.interaction.interactiontestproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="totoro.project.interaction.interactiontestproject.SettingActivity">
        </activity>
        <activity android:name="totoro.project.interaction.interactiontestproject.BeforeTestActivity">
        </activity>
        <activity android:name="totoro.project.interaction.interactiontestproject.PointActivity">
        </activity>
        <activity android:name="totoro.project.interaction.interactiontestproject.SwipeActivity">
        </activity>
        <activity android:name="totoro.project.interaction.interactiontestproject.SlideNotiActivity">
        </activity>
    </application>

</manifest>